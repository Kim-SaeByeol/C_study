#include <stdio.h>
	void main() {
	
	int start, end, multiple;
	printf("합계의 시작값 ==> ");
	scanf("%d", &start);

	printf("합계의 끝값 ==> ");
	scanf("%d", &end);

	printf("배수 ==> ");
	scanf("%d", &multiple);

	int sum = 0;
	for (int i = start; i <= end; i++) {
		if (i % multiple == 0)
		sum += i;
	}
	printf("%d 부터 %d까지의 %d배수의 합계 ==> %d\n", start, end, multiple, sum);
}
